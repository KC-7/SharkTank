<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kkmp.sharktank">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login"/>

        <activity android:name=".C_Submit_Info"/>
        <activity android:name=".C_Submitted_Info"/>

        <activity android:name=".R_Submit_Info"/>
        <activity android:name=".R_Submit_Emergency_Info"/>
        <activity android:name=".R_Dashboard"/>
        <activity android:name=".R_Make_Request"/>
        <activity android:name=".R_Browse_Listings"/>

    </application>

</manifest>